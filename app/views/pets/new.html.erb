<h1>Pets#new</h1>
<p>Find me in app/views/pets/new.html.erb</p>

<%= form_with(url: "#", model: @pet, local: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :birthdate %>
  <%= f.date_field :birthdate %>
  <%= f.label :breed %>
  <%= f.text_field :breed %>
    <%= f.fields_for :kind do |kind_f| %>
      <%= kind_f.label :kind %>
      <%= kind_f.text_field :description %> 
    <% end %>
  <% end %>